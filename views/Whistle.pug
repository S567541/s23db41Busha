extends layout

block content
  
  h1= title
  - let results=whistlelist
  div(class="container-fluid")
  div(class="row")
   each value,index in results
    div(class="col-sm-2" onclick=`buy(${index})`)  Whistle ID:#{value.WhistleId} , price : #{value.Whistle_Price}, Style: #{value.Whistle_Style}
   script.
    function buy(index)
    {
      alert(`Your purchased Item ${index}`)
    }